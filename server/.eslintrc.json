{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "error",
        "no-undef": "error",
        "indent": ["error", 2],
        "quotes": ["error", "single", "avoid-escape"],
        "curly": "error",
        "no-extra-semi": "warn"
    }
}
